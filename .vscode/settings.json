{
    "javascript.validate.enable": true,
    "editor.insertSpaces": true,
    "editor.tabSize": 4,
    "editor.trimAutoWhitespace": true,
    "jslint.enable": true,
    "jslint.options": {
        "browser": true,
        "devel": true,
        "es6": true,
        "indent": 4,
        "multivar": true,
        "predef": ["$", "_", "alert", "window"]
    },
    "jslint.version": "latest",
    "spellright.language": "en",
    "spellright.documentTypes": [
        "markdown",
        "latex",
        "plaintext",
        "typescript"
    ],
    "cSpell.words": [
        "CSGO",
        "Deathmatch",
        "gamesvr",
        "Laclede's",
        "lacledeslan",
        "LLTV",
        "mapgroup",
        "Maplist",
        "maxplayers",
        "rcon",
        "relaypassword",
        "teamname",
        "tickrate",
        "usercon"
    ]
}
